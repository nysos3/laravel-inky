{
  "name": "nysos3/laravel-inky",
  "description": "Foundation Inky email templates in Laravel",
  "keywords": [
    "foundation",
    "inky",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Cody Carrell",
      "email": "nysos3@gmail.com"
    }
  ],
  "require": {
    "nysos3/inky": "^1.3.8",
    "illuminate/support": "~5.7|~6.0|~7.0",
    "illuminate/view": "~5.7|~6.0|~7.0",
    "symfony/css-selector": "^2.7|^3.0|^4.0",
    "symfony/dom-crawler": "^2.7|^3.0|^4.0",
    "tijsverkoyen/css-to-inline-styles": "^2.2"
  },
  "require-dev": {
    "graham-campbell/testbench": "^3.3",
    "phpunit/phpunit": "~5.4",
    "mockery/mockery": "^0.9.5"
  },
  "autoload": {
    "psr-4": {
      "Petecoop\\LaravelInky\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Petecoop\\Tests\\LaravelInky\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Petecoop\\LaravelInky\\InkyServiceProvider"
      ]
    }
  }
}
